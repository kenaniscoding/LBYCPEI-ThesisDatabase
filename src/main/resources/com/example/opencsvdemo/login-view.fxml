<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="1024.2" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.opencsvdemo.LoginController">
   <children>
      <Label layoutX="260.0" layoutY="343.0" prefHeight="27.0" prefWidth="142.0" text="Username">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="260.0" layoutY="396.0" prefHeight="27.0" prefWidth="142.0" text="Password">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="404.0" layoutY="344.0">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></TextField>
      <Button layoutX="260.0" layoutY="448.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="34.0" prefWidth="142.0" text="Login">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="462.0" layoutY="448.0" mnemonicParsing="false" onAction="#onExit" prefHeight="34.0" prefWidth="100.0" text="Exit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <PasswordField fx:id="password" layoutX="404.0" layoutY="397.0" prefHeight="25.0" prefWidth="160.0" />
      <Label fx:id="prompt" alignment="TOP_LEFT" contentDisplay="CENTER" layoutX="125.0" layoutY="286.0" prefHeight="42.0" prefWidth="652.0" textFill="#e60000">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Label>
   </children>
</AnchorPane>
